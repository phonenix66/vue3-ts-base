# top-most EditorConfig file 表示是最顶层的配置文件，发现设为true时，才会停止查找.editorconfig文件
root = true
[*.{js,jsx,ts,tsx,vue}]
charset = utf-8
indent_size = 4
trim_trailing_whitespace = true
insert_final_newline = true
 
# Unix-style newlines with a newline ending every file 对于所有的文件 始终在文件末尾插入一个新行
[*]
end_of_line = lf
insert_final_newline = true
 
# 对于所有的js文件，设置文件字符集为utf-8
[*.js,*.ts,*vue]
charset = utf-8
 
# 设置所有JS,vue的缩进为
[*.{js,vue}]
indent_style=space
indent_size=4
[*.svg]
indent_style=space
indent_size=4

[*.js.map]
indent_style=space
indent_size=4

[*.less]
indent_style=space
indent_size=4

[*.vue]
indent_style=space
indent_size=4
[{.babelrc,.stylelintrc,jest.config,.eslintrc,.prettierrc,*.json,*.jsb3,*.jsb4,*.bowerrc}]
indent_style=space
indent_size=4
